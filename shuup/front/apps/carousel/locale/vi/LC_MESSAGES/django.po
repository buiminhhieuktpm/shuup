msgstr "Lọc theo tên ..."

#: front/apps/carousel/forms.py:21 front/apps/carousel/models.py:116
msgid "Carousel"
msgstr "Băng chuyền"

#: front/apps/carousel/forms.py:26
msgid "Active"
msgstr "Tích cực"

#: front/apps/carousel/models.py:32 front/apps/carousel/models.py:246
#: front/apps/carousel/models.py:249
msgid "Slide"
msgstr "Cầu trượt"

#: front/apps/carousel/models.py:33
msgid "Fade"
msgstr "Phai"

#: front/apps/carousel/models.py:41
msgid "Current page"
msgstr "Trang hiện tại"

#: front/apps/carousel/models.py:42
msgid "New page"
msgstr "Trang mới"

#: front/apps/carousel/models.py:69
msgid "shops"
msgstr "Cửa hàng"

#: front/apps/carousel/models.py:70
msgid "Select which shops you would like the carousel to be visible in."
msgstr "Chọn những cửa hàng nào bạn muốn hiển thị băng chuyền."

#: front/apps/carousel/models.py:73 front/apps/carousel/models.py:131
msgid "name"
msgstr "tên"

#: front/apps/carousel/models.py:73
msgid "The carousel name use for carousel configuration."
msgstr "Tên băng chuyền sử dụng cho cấu hình băng chuyền."

#: front/apps/carousel/models.py:78
msgid "animation"
msgstr "hoạt hình"

#: front/apps/carousel/models.py:79
msgid "Animation type for cycling slides."
msgstr "Loại hoạt hình cho các slide đạp xe."

#: front/apps/carousel/models.py:81
msgid "interval"
msgstr "khoảng thời gian"

#: front/apps/carousel/models.py:81
msgid "Slide interval in seconds."
msgstr "Khoảng thời gian trượt trong giây."

#: front/apps/carousel/models.py:84
msgid "pause on hover"
msgstr "Tạm dừng trên di chuột"

#: front/apps/carousel/models.py:85
msgid "When checked, the carousel cycling pauses on mouse over."
msgstr "Khi được kiểm tra, các băng chuyền đạp xe tạm dừng trên chuột."

#: front/apps/carousel/models.py:89
msgid "show navigation arrows"
msgstr "Hiển thị mũi tên điều hướng"

#: front/apps/carousel/models.py:91
msgid ""
"When checked, navigational arrows are shown on the carousel allowing for "
"customers to go back and forward."
msgstr ""

#: front/apps/carousel/models.py:96
msgid "show navigation dots"
msgstr "Hiển thị các chấm điều hướng"

#: front/apps/carousel/models.py:97
msgid "When checked, navigational indicator dots are shown."
msgstr "Khi được kiểm tra, các dấu chấm chỉ báo điều hướng được hiển thị."

#: front/apps/carousel/models.py:100
msgid "image width"
msgstr "chiều rộng hình ảnh"

#: front/apps/carousel/models.py:100
msgid "Slide images will be cropped to this width."
msgstr "Hình ảnh trượt sẽ được cắt theo chiều rộng này."

#: front/apps/carousel/models.py:103
msgid "image height"
msgstr "chiều cao hình ảnh"

#: front/apps/carousel/models.py:103
msgid "Slide images will be cropped to this height."
msgstr "Hình ảnh trượt sẽ được cắt đến chiều cao này."

#: front/apps/carousel/models.py:106
msgid "Arrows color"
msgstr "Mũi tên màu"

#: front/apps/carousel/models.py:109
msgid "Set the custom color for the arrows."
msgstr "Đặt màu tùy chỉnh cho các mũi tên."

#: front/apps/carousel/models.py:132
msgid "Name is only used to configure slides."
msgstr "Tên chỉ được sử dụng để định cấu hình các slide."

#: front/apps/carousel/models.py:139
msgid "product link"
msgstr "Liên kết sản phẩm"

#: front/apps/carousel/models.py:140
msgid ""
"Set the product detail page that should be shown when this slide is clicked, "
"if any."
msgstr ""

#: front/apps/carousel/models.py:148
msgid "category link"
msgstr "Liên kết danh mục"

#: front/apps/carousel/models.py:149
msgid ""
"Set the product category page that should be shown when this slide is "
"clicked, if any."
msgstr ""

#: front/apps/carousel/models.py:155
msgid "cms page link"
msgstr "Liên kết trang CMS"

#: front/apps/carousel/models.py:158
msgid ""
"Set the web page that should be shown when the slide is clicked, if any."
msgstr ""

#: front/apps/carousel/models.py:165
msgid "ordering"
msgstr "đặt hàng"

#: front/apps/carousel/models.py:167
msgid ""
"Set the numeric order in which this slide should appear relative to other "
"slides in this carousel."
msgstr ""

#: front/apps/carousel/models.py:173
msgid "link target"
msgstr "liên kết mục tiêu"

#: front/apps/carousel/models.py:174
msgid ""
"Set this to current if clicking on this slide should open a new browser tab."
msgstr ""

#: front/apps/carousel/models.py:179
msgid "available since"
msgstr "Có sẵn kể từ đó"

#: front/apps/carousel/models.py:181
msgid ""
"Set the date and time, starting from which this slide should be visible in "
"the carousel. This is useful to advertise sales campaigns or other time-"
"sensitive marketing."
msgstr ""

#: front/apps/carousel/models.py:188
msgid "available until"
msgstr "Có sẵn cho đến khi"

#: front/apps/carousel/models.py:190
msgid ""
"Set the date and time, until which this slide should be visible in the "
"carousel. This is useful to advertise sales campaigns or other time-"
"sensitive marketing."
msgstr ""

#: front/apps/carousel/models.py:195
msgid "Inactive dot border color"
msgstr "Màu đường viền chấm không hoạt động"

#: front/apps/carousel/models.py:198
msgid "Customize the dot border color when slide is not active."
msgstr "Tùy chỉnh màu đường viền chấm khi slide không hoạt động."

#: front/apps/carousel/models.py:201
msgid "Active dot color"
msgstr "Màu chấm hoạt động"

#: front/apps/carousel/models.py:204
msgid "Customize the dot color when slide is active."
msgstr "Tùy chỉnh màu chấm khi slide hoạt động."

#: front/apps/carousel/models.py:212
msgid "caption"
msgstr "đầu đề"

#: front/apps/carousel/models.py:214
msgid ""
"Text that describes the image. It is displayed on top of the image if "
"'Render image text' box is enabled in front-end. Also used for search engine "
"purposes."
msgstr ""

#: front/apps/carousel/models.py:221
msgid "caption text"
msgstr "Chú thích văn bản"

#: front/apps/carousel/models.py:223
msgid ""
"Caption text is displayed as secondary text on top of the image if 'Render "
"image text' box is enabled in front-end for 'Carousel plugin' type (disabled "
"for 'Banner box' type). It is also shown as a tooltip."
msgstr ""

#: front/apps/carousel/models.py:232
msgid "external link"
msgstr "liên kết bên ngoài"

#: front/apps/carousel/models.py:233
msgid ""
"Set the external site that should be shown when this slide is clicked, if "
"any."
msgstr ""

#: front/apps/carousel/models.py:239
msgid "image"
msgstr "hình ảnh"

#: front/apps/carousel/models.py:241
msgid "The slide image to show."
msgstr "Hình ảnh slide để hiển thị."

#: front/apps/carousel/models.py:250
msgid "Slides"
msgstr "Slide"

#: front/apps/carousel/plugins.py:20
msgid "Carousel Plugin"
msgstr "Plugin băng chuyền"

#: front/apps/carousel/plugins.py:28 front/apps/carousel/plugins.py:75
msgid "Render image text"
msgstr "Kết xuất văn bản hình ảnh"

#: front/apps/carousel/plugins.py:31 front/apps/carousel/plugins.py:78
msgid "Display the text on top of the image."
msgstr "Hiển thị văn bản trên đầu hình ảnh."

#: front/apps/carousel/plugins.py:68
msgid "Banner Box"
msgstr "Hộp biểu ngữ"

#: front/apps/carousel/plugins.py:71
msgid "Title"
msgstr "Tiêu đề"
